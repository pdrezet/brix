clean: 
	rm -rf ./.deps ./.pch
	rm -f ./*.o
	-(cd cube && $(MAKE) clean)
	-(cd isosurf && $(MAKE) clean)
	-(cd penguin && $(MAKE) clean)

distclean: clean
	rm -f config.cache config.log config.status bk-deps bk-make-pch shared-ld-sh Makefile
	-(cd cube && $(MAKE) distclean)
	-(cd isosurf && $(MAKE) distclean)
	-(cd penguin && $(MAKE) distclean)

